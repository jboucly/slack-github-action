{
    "version": 2,
    "name": "webhook-github-action",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/(.*)",
            "use": "@vercel/node"
        }
    ],
    "routes": [{ "src": "/(.*)", "dest": "/server.js" }]
}

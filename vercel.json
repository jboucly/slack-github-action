{
    "version": 2,
    "name": "webhook-github-action",
    "builds": [
        {
            "src": "./server.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "/.*",
            "dest": "server.js"
        }
    ]
}

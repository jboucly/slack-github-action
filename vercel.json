{
    "version": 2,
    "name": "webhook-github-action",
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        },
        {
            "src": "./**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/github-actions/slack",
            "dest": "/server.js"
        },
        {
            "src": "/github-actions/discord",
            "dest": "/server.js"
        }
    ]
}
